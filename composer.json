{
    "name": "nickurt/laravel-plesk",
    "description": "Plesk for Laravel 5",
    "keywords": ["plesk", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.5.0|~5.6.0",
        "plesk/api-php-lib": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "nickurt\\Plesk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "nickurt\\Plesk\\ServiceProvider"
            ],
            "aliases": {
                "Plesk": "nickurt\\Plesk\\Facade"
            }
        }
    }
}